{
  "name": "push2cloud-example-host",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.babel.js",
    "build": "npm run clean && npm run build:webpack",
    "check": "npm run lint && npm run test",
    "clean": "rimraf assets coverage",
    "lint": "eslint --ext=.js,.jsx js/ test/ *.js",
    "start:dev": "NODE_ENV=development ./node_modules/.bin/babel-node server.js",
    "start": "NODE_ENV=production ./node_modules/.bin/babel-node server.js",
    "test:cov": "NODE_ENV=test babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "test:watch": "NODE_ENV=test npm run test -- --watch --growl",
    "test": "NODE_ENV=test mocha --recursive --compilers=js:babel-register --require=./test/helpers/jsdom.js"
  },
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "deployment": {
    "disk": "1024M",
    "appConnections": {
      "push2cloud-example-api": {}
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.6.1",
    "camel-case": "^1.2.2",
    "cfenv": "^1.0.3",
    "classnames": "^2.2.3",
    "constant-mirror": "^1.0.6",
    "fetch-check-http-status": "^1.0.0",
    "http-proxy-middleware": "^0.13.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "react": "^0.14.7",
    "react-dnd": "^2.1.2",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "2.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "todomvc-app-css": "^2.0.4",
    "morgan": "^1.7.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "babel-cli": "^6.6.5",
    "babel-runtime": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "webpack-hot-middleware": "^2.9.1",
    "express": "^4.13.4"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-register": "^6.6.5",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.0",
    "expect.js": "^0.3.1",
    "fetch-mock": "^4.1.1",
    "isparta": "^4.0.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.7",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-mock-store": "^0.0.6",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0"
  }

}
